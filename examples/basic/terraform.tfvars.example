# Example terraform.tfvars for RunsOn deployment
# Copy this file to terraform.tfvars and fill in your values

# AWS Configuration
aws_region   = "us-east-1"
stack_name   = "runs-on" # Must be unique in your account/region
environment  = "production"

# VPC Configuration (adjust as needed)
vpc_cidr                  = "10.0.0.0/16"
public_subnet_cidrs       = ["10.0.0.0/20", "10.0.16.0/20", "10.0.32.0/20"]
private_subnet_cidrs      = ["10.0.128.0/20", "10.0.144.0/20", "10.0.160.0/20"]

# Required: RunsOn Configuration
github_organization = "your-github-org"        # TODO: Replace with your GitHub org/username
license_key         = "YOUR-LICENSE-KEY-HERE"  # TODO: Replace with your RunsOn license key
email               = "admin@example.com"      # TODO: Replace with your email (optional)

ssh_allowed    = true          # Allow SSH access to runners
ssh_cidr_range = "0.0.0.0/0"  # Restrict to your IP range for better security
